    <h1 style="margin-top: 20px;">
        <p style="text-align: center;">TRACK LEAVE
        </p>
    </h1>
    <div class="MIDDLE" style="width: 80%;">


        <table class="table table-striped" style="border:rgb(84, 75, 75); ">
            <thead>
                <tr>
                    <td>
                        <h4 style="color: gray;">Name</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">EmployeeId</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">Email</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">Leave Type</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">From Date</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">To Date</h4>
                    </td>
                    <td>
                        <h4 style="color: gray;">Status</h4>
                    </td>
                </tr>
            </thead>
            <% records.forEach(function(data){%>
                <span style="visibility: hidden;">
                    <%= count=0 %>
                </span>
                <tr>
                    <td>
                        <%= data.FirstName %>
                    </td>
                    <td>
                        <%= data.EmployeeId%>
                    </td>
                    <td>
                        <%= data.EmailId %>
                    </td>
                    <td>
                        <%= data.LeaveType %>
                    </td>
                    <td>
                        <%= data.FromDate%>
                    </td>
                    <td>
                        <%= data.ToDate %>
                    </td>
                    <td>
                        <%= data.Status %>
                    </td>
                </tr>
                <%});%>
        </table>

